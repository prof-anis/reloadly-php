{
    "name": "busybrain/reloadly",
    "description": "A simple php library to interact with the reloadly api",
    "type": "library",
    "license": "mit",
    "autoload":{
        "psr-4":{
            "Busybrain\\Reloadly\\" : "src/",
            "Busybrain\\Reloadly\\Test\\" : "test/"
        }
    },
    "authors": [
        {
            "name": "prof-anis",
            "email": "tobexkee@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/collections": "^8.1",
        "illuminate/container": "^8.1",
        "illuminate/support": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/http": "^8.1",
        "phpunit/phpunit": "^9.4",
        "mockery/mockery": "^1.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18"
    },
    "scripts": {
        "fix-style":"php-cs-fixer fix --verbose"
    }

}
